<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.InternetShopAndroid" parent="Theme.MaterialComponents.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/app_newblack</item>
        <item name="colorPrimaryVariant">@color/black</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/app_greyscale_line</item>
        <item name="colorSecondaryVariant">@color/app_greyscale_label</item>
        <item name="colorOnSecondary">@color/app_greyscale_body</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorOnBackground</item>
        <!-- Customize your theme here. -->
<!--        <item name="actionBarStyle">@style/Toolbar.Title</item>-->
    </style>
</resources>